#strict superset has atleast one element that does not exist in its subset

set_a = set(map(int, input().split()))
n = int(input())
is_strict_superset = True

for i in range(n):
    set_b = set(map(int, input().split()))
    
    if not set_b.issubset(set_a) and len(set_a) > len(set_b):
        is_strict_superset = False
        break
            
print(is_strict_superset)

    
