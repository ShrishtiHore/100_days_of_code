from collections import OrderedDict

'''
>>> from collections import OrderedDict
>>> 
>>> ordinary_dictionary = {}
>>> ordinary_dictionary['a'] = 1
>>> ordinary_dictionary['b'] = 2
>>> ordinary_dictionary['c'] = 3
>>> ordinary_dictionary['d'] = 4
>>> ordinary_dictionary['e'] = 5
>>> 
>>> print ordinary_dictionary
{'a': 1, 'c': 3, 'b': 2, 'e': 5, 'd': 4}


>>> ordered_dictionary = OrderedDict()
>>> ordered_dictionary['a'] = 1
>>> ordered_dictionary['b'] = 2
>>> ordered_dictionary['c'] = 3
>>> ordered_dictionary['d'] = 4
>>> ordered_dictionary['e'] = 5
>>> 
>>> print ordered_dictionary
OrderedDict([('a', 1), ('b', 2), ('c', 3), ('d', 4), ('e', 5)])
'''

N = int(input())

ordered_items = OrderedDict()

for _ in range(N):
    item_line = input().split()
    
    # The price is the last element in the list
    price = int(item_line[-1])
    
    # The item name is all elements except the last one
    item_name = ' '.join(item_line[:-1])
    
    current_price = ordered_items.get(item_name, 0) #get name for easy addition
    
    ordered_items[item_name] = current_price + price
    
for name, net_price in ordered_items.items():
    print(name, net_price)











