if __name__ == '__main__':
    n = int(input())
    arr = map(int, input().split())
    my_list = list(arr) #converting the array to a list
    
    #removing duplicates from the list
    unique_list = []
    for item in my_list:
        if item not in unique_list:
            unique_list.append(item)
    unique_list.sort(reverse = True) #sorting the array
    
    second_highest = unique_list[1] #getting the second highest value
    print(second_highest)
    
