def merge_the_tools(string, k):
    n = len(string)
    
    for i in range(0, n, k):
        #Get the current substring (t) using slicing
        t = string[i:i+k]
        
        #Use a temporary list to store characters in the order of first appearance
        result = []
        
        #iterate through substring t
        for char in t:
            if char not in result:
                result.append(char)
                
        #Join the unique characters and print the final subsequence
        print("".join(result))
    
if __name__ == '__main__':
    string, k = input(), int(input())
    merge_the_tools(string, k)
