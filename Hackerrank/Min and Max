import numpy as np
import sys

N, M = map(int, sys.stdin.readline().split())

data_list = []
for _ in range(N):
    line = sys.stdin.readline().split()
    if line:
        data_list.extend(map(int, line))
    
if N > 0 and M > 0 and len(data_list) == N * M:
    numpy_array = np.array(data_list, dtype=int).reshape(N, M)
else:
    numpy_array = np.array([[]])
    
#axis = 1 means minimum value in vertical direction
#axis = 2 means minimum value in horizontal direction
#axis = 3 means minimum value overall
#then all the results form a 1D array 
min_array = np.min(numpy_array, axis=1) 

print(np.max(min_array))
