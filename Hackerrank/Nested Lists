if __name__ == '__main__':
    records = []
    
    for i in range(int(input())):
        name = input()
        score = float(input())
        records.append([name, score])
        
    # Extract all scores using a list comprehension
    all_scores = [record[1] for record in records]
    
    # Use set() to get only unique scores, then sort them.
    # The second element (index 1) is the second-lowest score.
    unique_scores = sorted(list(set(all_scores)))
    second_lowest_score = unique_scores[1]
    
    # Filter the names whose score matches the second-lowest, 
    # then sort the names alphabetically.
    second_lowest_students = sorted(
        [name for name, score in records if score == second_lowest_score]
    )
    
    # Print each name on a new line
    for name in second_lowest_students:
        print(name)
    
    
